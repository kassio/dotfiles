#!/usr/bin/env zsh

if [ "$1" = "--help" ]; then
  echo "Upgrade zsh_plugins"
  exit
fi

title "updating zsh_plugins"

source "${XDG_CONFIG_HOME}/antidote/antidote.zsh"

# uncomment if you want your session to have commands like `antidote update`
autoload -Uz "${install_path}/functions/antidote"

antidote update
