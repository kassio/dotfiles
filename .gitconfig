[core]
	excludesfile = ~/.gitignore
	editor = vim
[help]
	format = web
	browser = gc
	autocorrect = 1
[browser "gc"]
	cmd = google-chrome %s
[push]
	default = current
[branch]
	autosetuprebase = always
[color]
	branch = auto
	diff = auto
	interactive = auto
	status = auto
	grep = auto
	ui = auto
	pager = 1
[color "decorate"]
	branch = 1
