#!/usr/bin/env bash

old_path="$PWD"
cd $HOME

$HOME/.dotfiles/bin/install-link-files
$HOME/.dotfiles/bin/install-zsh
$HOME/.dotfiles/bin/install-osx
$HOME/.dotfiles/bin/update-git-prompt

if [ -e "$HOME/.zshrc" ]
then
  source $HOME/.zshrc &>/dev/null
elif [ -e "$HOME/.bashrc" ]
  source $HOME/.bashrc &>/dev/null
fi

cd $old_path
