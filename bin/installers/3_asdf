#!/usr/bin/env bash

for plugin in $(cat "${DOTFILES}/lib/asdf_default_plugins"); do
  title "Ensure to have ${plugin} installed"
  asdf plugin add "${plugin}"
  title "Ensure latest version of ${plugin}"
  asdf install "${plugin}" latest
  title "Ensure global version of ${plugin} to be latest"
  asdf global "${plugin}" latest
done
