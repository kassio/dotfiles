{
  "repositories": {
    "dotfiles": {
      "remote": "https://github.com/kassio/dotfiles",
      "local": "$HOME/.dotfiles",
      "afterSync": [
        "git",
        "switch",
        "main"
      ]
    },
    "data backup": {
      "remote": "https://github.com/kassio/sync",
      "local": "$HOME/src/sync"
    },
    "repos_syncer": {
      "remote": "https://gitlab.com/kassio/repos_syncer.git",
      "local": "$HOME/src/repos_syncer"
    },
    "blog": {
      "remote": "https://gitlab.com/kassio/kassio",
      "local": "$HOME/src/blog"
    },
    "gitlab xbar": {
      "remote": "https://gitlab.com/kassio/gitlab-xbar.git",
      "local": "$HOME/src/gdk/gitlab-xbar"
    }
  }
}
