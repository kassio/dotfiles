{
  "settings": {
    "colors": true
  },
  "repositories": {
    "dotfiles": {
      "remote": "https://github.com/kassio/dotfiles",
      "local": "$HOME/.dotfiles"
    },
    "syncer": {
      "remote": "https://github.com/kassio/sync",
      "local": "$HOME/src/sync"
    },
    "gsink": {
      "remote": "https://gitlab.com/kassio/gsink.git",
      "local": "$HOME/src/go/src/gsink"
    },
    "blog": {
      "remote": "https://gitlab.com/kassio/kassio",
      "local": "$HOME/src/blog"
    },
    "gitlab xbar": {
      "remote": "https://gitlab.com/kassio/gitlab-xbar.git",
      "local": "$HOME/src/gdk/gitlab-xbar"
    }
  }
}
