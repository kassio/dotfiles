\set PROMPT1 '%[%033[1m%]%/ %R%[%033[0m%]%# '
\set PROMPT2 ' %R > '

\set ON_ERROR_ROLLBACK interactive
\set COMP_KEYWORD_CASE upper
\set HISTFILE ~/.cache/psql-history- :DBNAME
\set HISTCONTROL ignoredups

\pset pager off
\pset tuples_only off
\pset null '(null)'

\timing

\if `[ -f ".psqlrc" ] && echo 1 || echo 0`
  \ir .psqlrc.local
\endif
