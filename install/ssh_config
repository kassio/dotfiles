Host github.com
  Hostname github.com
  IdentityFile ~/.ssh/git_id_rsa
  IdentitiesOnly yes

Host gist.github.com
  Hostname gist.github.com
  IdentityFile ~/.ssh/git_id_rsa
  IdentitiesOnly yes

Host bitbucket.org
  Hostname bitbucket.org
  IdentityFile ~/.ssh/git_id_rsa
  IdentitiesOnly yes

Host *
  UseKeychain yes
  AddKeysToAgent yes
  IdentityFile ~/.ssh/id_rsa
